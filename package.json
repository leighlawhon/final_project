{
  "private": true,
  "version": "0.0.1",
  "description": "ASCII Wearhouse Test for X-Team",
  "author": "Leigh Lawhon",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "rule-empty-line-before": "always"
    }
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "eslint": "eslint ./src; exit 0",
    "stylelint": "stylelint \"./src/components/**/*.css\" \"./src/styles/**/*.css\"; exit 0",
    "lint": "npm run eslint && npm run stylelint",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "concurrently --kill-others  \"npm run startASCII\" \"node server.js --env=dev\"",
    "startASCII": "node index.js",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": "",
  "keywords": [],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-add-jsdoc-properties": "^0.1.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "bower-webpack-plugin": "^0.1.9",
    "concurrently": "^3.4.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-webpack-plugin": "0.0.2",
    "karma": "~1.4.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^3.0.0",
    "mochawesome": "^2.0.4",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^1.2.9",
    "rimraf": "^2.4.3",
    "should": "^11.2.0",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.0",
    "stylelint": "^7.0.3",
    "stylelint-config-standard": "^11.0.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "cool-ascii-faces": "^1.3.4",
    "core-js": "^2.0.0",
    "ecstatic": "^1.4.0",
    "karma-should": "^1.0.0",
    "lodash": "^4.17.4",
    "normalize.css": "^4.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
